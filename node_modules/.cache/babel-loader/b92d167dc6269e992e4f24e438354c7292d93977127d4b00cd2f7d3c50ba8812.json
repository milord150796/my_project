{"ast":null,"code":"export default {\n  name: 'LaserShow'\n};","map":{"version":3,"names":["name"],"sources":["/Users/admin/Documents/github/my_project/src/components/Lazer.vue"],"sourcesContent":["<template>\n    <div class=\"laser-show\">\n      <h1>Лазерне Шоу!</h1>\n      <p>Насолоджуйтеся яскравим лазерним шоу!</p>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    name: 'LaserShow',\n  };\n  </script>\n  \n  <style scoped>\n  .laser-show {\n    position: relative;\n    width: 100%;\n    height: 100vh;\n    overflow: hidden;\n    text-align: center;\n    color: white;\n    background: #000;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    font-family: Arial, sans-serif;\n  }\n  \n  .laser-show h1 {\n    font-size: 4rem;\n    margin-bottom: 20px;\n  }\n  \n  .laser-show p {\n    font-size: 1.5rem;\n  }\n  \n  /* Лазерні промені */\n  .laser-show::before,\n  .laser-show::after {\n    content: '';\n    position: absolute;\n    width: 200%;\n    height: 200%;\n    background: linear-gradient(\n      45deg,\n      rgba(255, 0, 0, 0.7),\n      rgba(0, 255, 0, 0.7),\n      rgba(0, 0, 255, 0.7),\n      rgba(255, 255, 0, 0.7)\n    );\n    background-size: 200% 200%;\n    animation: laserAnimation 5s linear infinite;\n  }\n  \n  .laser-show::before {\n    top: -50%;\n    left: -50%;\n    transform: rotate(45deg);\n  }\n  \n  .laser-show::after {\n    top: -50%;\n    left: -50%;\n    transform: rotate(-45deg);\n  }\n  \n  @keyframes laserAnimation {\n    0% {\n      background-position: 0% 0%;\n    }\n    50% {\n      background-position: 100% 100%;\n    }\n    100% {\n      background-position: 0% 0%;\n    }\n  }\n  </style>\n  "],"mappings":"AAQE,eAAe;EACbA,IAAI,EAAE;AACR,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}