         this.hasWeapon = true;\n            }, 6000); // час до взяття зброї\n\n            setTimeout(() => {\n                this.showDonateMessage = true;\n            }, 8000); // час до появи повідомлення\n        }\n    }\n};\n</script>\n\n<style scoped>\n.animation-container {\n    position: relative;\n    height: 100vh;\n    overflow: hidden;\n}\n\n.plane {\n    position: absolute;\n    top: 10%;\n    left: 10%;\n    width: 100px;\n    height: 60px;\n    background: url('../assets/airplane-background.webp') no-repeat center center;\n    background-size: contain;\n    animation: planeFly 6s linear infinite;\n}\n\n.parachute {\n    position: absolute;\n    top: 50%;\n    left: 30%;\n    width: 50px;\n    height: 80px;\n    background: url('../assets/airplane-background.webp') no-repeat center center;\n    background-size: contain;\n    animation: parachuteDescend 3s ease-out forwards;\n}\n\n.person {\n    position: absolute;\n    top: 70%;\n    left: 30%;\n    width: 40px;\n    height: 80px;\n    background: url('../assets/airplane-background.webp') no-repeat center center;\n\n    animation: personAnimate 3s ease-out forwards;\n}\n\n.weapon {\n    position: absolute;\n    top: 75%;\n    left: 30%;\n    width: 40px;\n    height: 60px;\n    background: url('../assets/airplane-background.webp') no-repeat center center;\n    background-size: contain;\n    animation: weaponAnimate 2s ease-out forwards;\n}\n\n.donate-message {\n    position: absolute;\n    bottom: 20%;\n    left: 50%;\n    transform: translateX(-50%);\n    font-size: 2rem;\n    color: white;\n    background: rgba(0, 0, 0, 0.7);\n    padding: 1rem;\n    border-radius: 10px;\n    animation: fadeIn 2s ease-out;\n}\n\n@keyframes planeFly {\n    0% {\n        transform: translateX(-100vw);\n    }\n\n    100% {\n        transform: translateX(100vw);\n    }\n}\n\n@keyframes parachuteDescend {\n    0% {\n        top: 10%;\n    }\n\n    100% {\n        top: 50%;\n    }\n}\n\n@keyframes personAnimate {\n   