ationFrame(loop);\n          }\n        };\n        loop();\n      },\n      isCupInBasket(cup) {\n        return cup.x < this.basketX + 100 && cup.x + 50 > this.basketX;\n      },\n      resetGame() {\n        this.startGame();\n      },\n    },\n  };\n  </script>\n  \n  <style scoped>\n  .game-container {\n    position: relative;\n    width: 100%;\n    height: 100vh;\n    overflow: hidden;\n    background-color: #f5f5f5;\n    text-align: center;\n  }\n  \n  .game-screen {\n    position: relative;\n  }\n  \n  .start-screen {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 100vh;\n  }\n  \n  .game-elements {\n    position: relative;\n    height: 100vh;\n  }\n  \n  .basket {\n    position: absolute;\n    bottom: 10px;\n    width: 100px;\n    height: 30px;\n    background-color: #4CAF50;\n    border-radius: 5px;\n  }\n  \n  .cup {\n    position: absolute;\n    width: 50px;\n    height: 50px;\n    background-color: #FF6347;\n  }\n  \n  .game-over {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    background-color: rgba(0, 0, 0, 0.8);\n    color: white;\n    padding: 20px;\n    border-radius: 10px;\n  }\n  </style>\n  "],"mappings":";;;EACSA,KAAK,EAAC;AAAgB;;EACpBA,KAAK,EAAC;AAAa;;EAF9BC,GAAA;EAGiCD,KAAK,EAAC;;;EAHvCC,GAAA;EAMgCD,KAAK,EAAC;;;EANtCC,GAAA;EAS+BD,KAAK,EAAC;;gEACzBE,mBAAA,CAAqB,WAAlB,gBAAc;;EAV7BD,GAAA;AAAA;;uBACIE,mBAAA,CAgBM,OAhBNC,UAgBM,GAfJF,mBAAA,CAaM,OAbNG,UAaM,G,CAZQC,KAAA,CAAAC,WAAW,I,cAAvBJ,mBAAA,CAEM,OAFNK,UAEM,GADJN,mBAAA,CAA8C;IAArCO,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,SAAA,IAAAD,QAAA,CAAAC,SAAA,IAAAF,IAAA,CAAS;KAAE,YAAU,E,KAJ/CG,mBAAA,gBAMmBR,KAAA,CAAAC,WAAW,I,cAAtBJ,mBAAA,CAQM,OARNY,UAQM,GAPJb,mBAAA,CAA4D;IAAvDF,KAAK,EAAC,QAAQ;IAAEgB,KAAK,EAPpCC,eAAA;MAAAC,IAAA,EAO8CZ,KAAA,CAAAa,OAAO;IAAA;8CAC3ChB,mBAAA,CAAoHiB,SAAA,QAR9HC,WAAA,CAQsCf,KAAA,CAAAgB,IAAI,EAR1C,CAQuBC,GAAG,EAAEC,KAAK;yBAAvBrB,mBAAA,CAAoH;MAAjFF,GAAG,EAAEuB,KAAK;MAAExB,KAA